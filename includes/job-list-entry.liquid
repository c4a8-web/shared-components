<!-- Shared Job List Entry Section -->
<div class="job-list__entry {% if include.isInvisible %}d-none{% endif %}">
  <div class="job-list__entry-header">
    {% if include.team %} {{ include.team }} / {% endif %} {% if include.positionType %} {{ include.positionType }} / {%
    endif %} {{ include.city }}
  </div>
  <div class="job-list__entry-content">
    <div class="job-list__entry-title">
      {{ include.title }}
      <span class="job-list__entry-gender">{{ include.gender }}</span>
    </div>
  </div>
</div>
<!-- End Shared Job List Entry Section -->
